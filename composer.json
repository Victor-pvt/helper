{
    "type": "symfony-bundle",
    "name": "victor-pvt/helper-manager",
    "description": "helper manager bundle",
    "license": "proprietary",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "HelperManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HelperManager\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
